{
  "builds": [
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/robots.txt", "dest": "/public/robots.txt" },
    {
      "src": "/private/admin.html",
      "has": [
        { "type": "query", "key": "key", "value": "RAMAN2025" }
      ],
      "dest": "/public/private/admin.html"
    },
    {
      "src": "/private/(.*)",
      "status": 403,
      "dest": "/"
    },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
